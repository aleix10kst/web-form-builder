    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "geist": "^1.3.1",
    "lucide-react": "^0.419.0",
    "nanoid": "^5.0.7",
    "next": "14.2.5",
    "next-themes": "^0.3.0",
